{
  "name": "Veripages",
  "url": "veripages.com",
  "version": "0.6.0",
  "addedDatetime": 1691989200000,
  "optOutUrl": "https://veripages.com/inner/control/privacy",
  "steps": [
    {
      "stepType": "scan",
      "scanType": "templatedUrl",
      "actions": [
        {
          "actionType": "navigate",
          "id": "658fce8a-8bc7-4fb7-9f52-4cdbf2e90054",
          "url": "https://veripages.com/inner/profile/search?fname=${firstName}&lname=${lastName}&fage=${age|ageRange}&state=${state}&city=${city}",
          "ageRange": [
            "18-30",
            "31-40",
            "41-50",
            "51-60",
            "61-70",
            "71-80",
            "81+"
          ]
        },
        {
          "actionType": "extract",
          "id": "4bb4d602-8697-4ad0-ab7e-fef2b5b8406c",
          "selector": ".search-item",
          "noResultsSelector": "//h1[contains(text(), 'Oops')]",
          "profile": {
            "name": {
              "selector": ".search-item-title",
              "beforeText": ", "
            },
            "alternativeNamesList": {
              "selector": ".//dt[contains(text(), 'Also known as')]/following-sibling::dd",
              "findElements": true
            },
            "age": {
              "selector": ".search-item-title",
              "afterText": ", "
            },
            "addressCityState": {
              "selector": ".search-item-loc",
              "findElements": true
            },
            "addressCityStateList": {
              "selector": ".//dt[contains(text(), 'Has lived in')]/following-sibling::dd/a",
              "findElements": true
            },
            "relativesList": {
              "selector": ".//dt[contains(text(), 'Related to')]/following-sibling::dd",
              "findElements": true
            },
            "profileUrl": {
              "selector": ".search-item-title-link",
              "identifierType": "path",
              "identifier": "https://veriforia.com/view/${id}"
            }
          }
        }
      ]
    },
    {
      "stepType": "optOut",
      "optOutType": "formOptOut",
      "actions": [
        {
          "actionType": "navigate",
          "url": "https://veripages.com/inner/control-privacy",
          "id": "764853d2-0735-4348-a6c6-039388b25094"
        },
        {
          "actionType": "expectation",
          "expectations": [
            {
              "type": "element",
              "selector": "//p[contains(text(), 'opt out')]",
              "parent": ".container"
            }
          ],
          "id": "837814e9-34b7-41d8-bd58-c670c1e07ccf"
        },
        {
          "actionType": "fillForm",
          "selector": ".ahm",
          "elements": [
            {
              "type": "fullName",
              "selector": "#name"
            },
            {
              "type": "email",
              "selector": "#email"
            },
            {
              "type": "profileUrl",
              "selector": "#url"
            }
          ],
          "id": "08b36264-34fe-469e-bc28-e7ccf46ffcaa"
        },
        {
          "actionType": "expectation",
          "expectations": [
            {
              "type": "element",
              "selector": "//input[@name='cf-turnstile-response' and @value and @value != '']",
              "parent": "//form[@id='privacynew']"
            }
          ],
          "id": "a403483f-8ee9-4a6a-92fb-b5bf8fc97df9"
        },
        {
          "actionType": "click",
          "elements": [
            {
              "type": "button",
              "selector": ".//button[@type='submit']"
            }
          ],
          "id": "e897bc2f-8460-4b7c-9ef4-979eda057f03"
        },
        {
          "actionType": "expectation",
          "expectations": [
            {
              "type": "text",
              "selector": "body",
              "expect": "Your removal request has been received"
            }
          ],
          "id": "bd99ed6f-bdfd-4c76-ba88-a3d068e8973d"
        },
        {
          "actionType": "emailConfirmation",
          "pollingTime": 30,
          "id": "98b82794-4c5c-40fa-b3c5-7b429e701daf"
        },
        {
          "actionType": "expectation",
          "expectations": [
            {
              "type": "text",
              "selector": "body",
              "expect": "Your information control request has been confirmed."
            }
          ],
          "id": "4623cdb6-ad7e-474a-9bdd-2c759ac6e735"
        }
      ]
    }
  ],
  "schedulingConfig": {
    "retryError": 48,
    "confirmOptOutScan": 72,
    "maintenanceScan": 120,
    "maxAttempts": -1
  },
  "removedAt": null
}
