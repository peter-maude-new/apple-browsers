{
  "name": "Dataveria",
  "url": "dataveria.com",
  "version": "0.6.0",
  "addedDatetime": 1677736800000,
  "optOutUrl": "https://dataveria.com/ng/control/privacy",
  "steps": [
    {
      "stepType": "scan",
      "scanType": "templatedUrl",
      "actions": [
        {
          "actionType": "navigate",
          "id": "92f18635-ddbb-4700-afcd-055f80f721b8",
          "url": "https://dataveria.com/profile/search?fname=${firstName}&lname=${lastName}&state=${state}&city=${city}&fage=${age|ageRange}",
          "ageRange": [
            "18-30",
            "31-40",
            "41-50",
            "51-60",
            "61-70",
            "71-80",
            "81+"
          ]
        },
        {
          "actionType": "extract",
          "id": "441a4974-cc5a-401d-aec5-f597956327ff",
          "selector": ".search-item",
          "noResultsSelector": "//div[contains(@class, 'page-404')]//h1[contains(text(), 'Sorry')]",
          "profile": {
            "name": {
              "selector": ".//h2[@class='search-item__title-age']",
              "beforeText": ","
            },
            "alternativeNamesList": {
              "selector": ".//div[@class='search-item__known-col']/dd[@class='search-item__known-info']",
              "findElements": true
            },
            "age": {
              "selector": ".//h2[@class='search-item__title-age']"
            },
            "addressCityState": {
              "selector": ".//div[@class='search-item__title-loc']",
              "afterText": "Addresses:"
            },
            "addressCityStateList": {
              "selector": ".//dt[@class='search-item__info-title' and contains(text(), 'Used to Live')]/following-sibling::div[@class='search-item__info-col']/dd",
              "findElements": true
            },
            "relativesList": {
              "selector": ".//dt[@class='search-item__info-title' and contains(text(), 'Relatives')]/following-sibling::div[@class='search-item__info-col']/dd",
              "findElements": true
            },
            "profileUrl": {
              "selector": "a",
              "identifierType": "path",
              "identifier": "https://dataveria.com/view/${id}"
            }
          }
        }
      ]
    },
    {
      "stepType": "optOut",
      "optOutType": "formOptOut",
      "actions": [
        {
          "actionType": "navigate",
          "url": "https://dataveria.com/opt-out/privacy",
          "id": "b7ff47f7-7466-4399-a144-606378d596dd"
        },
        {
          "actionType": "expectation",
          "expectations": [
            {
              "type": "element",
              "selector": "//h1[contains(text(), 'Opt Out')]",
              "parent": ".container"
            }
          ],
          "id": "98988ee9-b838-4fa3-b1f2-b4f35e77be25"
        },
        {
          "actionType": "fillForm",
          "selector": ".ahm",
          "elements": [
            {
              "type": "fullName",
              "selector": "#user_name"
            },
            {
              "type": "email",
              "selector": "#user_email"
            },
            {
              "type": "profileUrl",
              "selector": "#url"
            }
          ],
          "id": "3406cb92-f8f3-47c8-865f-dd1dc2962f46"
        },
        {
          "actionType": "expectation",
          "expectations": [
            {
              "type": "element",
              "selector": "//input[@name='cf-turnstile-response' and @value and @value != '']",
              "parent": "//form[@id='form-contact']"
            }
          ],
          "id": "43fa7095-e26c-4360-8b1c-21a9c59fdaf8"
        },
        {
          "actionType": "click",
          "elements": [
            {
              "type": "button",
              "selector": ".//button[@type='submit']"
            }
          ],
          "id": "502343a6-a543-4751-9a4f-4503d1531008"
        },
        {
          "actionType": "expectation",
          "expectations": [
            {
              "type": "text",
              "selector": "body",
              "expect": "Your removal request has been received"
            }
          ],
          "id": "efdfc904-12aa-4af5-a316-efbed99e4c93"
        },
        {
          "actionType": "emailConfirmation",
          "pollingTime": 30,
          "id": "d2c74394-fb1d-4428-b7b0-4155de0c65ea"
        },
        {
          "actionType": "expectation",
          "expectations": [
            {
              "type": "text",
              "selector": "body",
              "expect": "Your information control request has been confirmed."
            }
          ],
          "id": "241d30c4-eba8-49d9-bd57-ab35ac2ce8cc"
        }
      ]
    }
  ],
  "schedulingConfig": {
    "retryError": 48,
    "confirmOptOutScan": 72,
    "maintenanceScan": 120,
    "maxAttempts": -1
  },
  "removedAt": null
}
