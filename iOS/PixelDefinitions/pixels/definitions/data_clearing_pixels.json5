{
    "m_fire_clearing_completion": {
        "description": "Fired when the data clearing operation completes successfully",
        "owners": ["hanyutang-sandra"],
        "triggers": ["user_submitted"],
        "suffixes": ["platform", "form_factor"],
        "parameters": [
            "appVersion",
            "pixelSource",
            {
                "key": "duration",
                "type": "integer",
                "description": "Total duration of the clearing operation in milliseconds"
            },
            {
                "key": "option",
                "type": "string",
                "description": "The clearing option selected by the user",
                "examples": ["close_tabs", "close_tabs_and_clear_data"]
            },
            {
                "key": "trigger",
                "type": "string",
                "description": "What triggered the clearing operation",
                "enum": ["fire_button", "single_tab_burn", "automatic"]
            },
            {
                "key": "scope",
                "type": "string",
                "description": "The scope of the clearing operation",
                "enum": ["single_tab", "all_tabs"]
            }
        ]
    },

    "m_fire_retrigger_in_20s": {
        "description": "Fired when the fire button is triggered again within 20 seconds of a previous fire",
        "owners": ["hanyutang-sandra"],
        "triggers": ["user_submitted"],
        "suffixes": ["daily_standard", "platform", "form_factor"],
        "parameters": ["appVersion", "pixelSource"]
    },

    "m_fire_user_action_before_completion": {
        "description": "Fired when user performs an action before the clearing operation completes",
        "owners": ["hanyutang-sandra"],
        "triggers": ["other"],
        "suffixes": ["platform", "form_factor"],
        "parameters": ["appVersion", "pixelSource"]
    },

    "m_fire_burn_tabs_duration": {
        "description": "Fired to report the duration of tabs clearing",
        "owners": ["hanyutang-sandra"],
        "triggers": ["other"],
        "suffixes": ["platform", "form_factor"],
        "parameters": [
            "appVersion",
            "pixelSource",
            {
                "key": "duration",
                "type": "integer",
                "description": "Duration of tabs clearing in milliseconds"
            },
            {
                "key": "scope",
                "type": "string",
                "description": "The scope of tabs being cleared",
                "enum": ["single_tab", "all_tabs"]
            }
        ]
    },

    "m_fire_burn_tabs_has_residue": {
        "description": "Fired when residual tab data is detected after clearing",
        "owners": ["hanyutang-sandra"],
        "triggers": ["other"],
        "suffixes": ["platform", "form_factor"],
        "parameters": ["appVersion", "pixelSource"]
    },

    "m_fire_burn_tabs_error": {
