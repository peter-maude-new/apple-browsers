// Data Clearing (Fire) pixels for tracking fire button performance and quality metrics
{
    // ==================== Overall Flow Metrics ====================

    "m_mac_fire_completion": {
        "description": "Fired when the fire (data clearing) operation completes successfully",
        "owners": ["hanyutang-sandra"],
        "triggers": ["user_submitted"],
        "parameters": [
            "appVersion",
            "pixelSource",
            {
                "key": "duration",
                "type": "integer",
                "description": "Total duration of the fire operation in milliseconds"
            },
            {
                "key": "clearing_option",
                "type": "string",
                "description": "The clearing option selected by the user",
                "enum": ["current_tab", "current_window", "all_data"]
            },
            {
                "key": "domains",
                "type": "string",
                "description": "Information about domains affected by the clearing operation",
                "examples": ["History", "History,TabsAndWindows", "CookiesAndSiteData", "History,TabsAndWindows,ChatHistory"]
            },
            {
                "key": "path",
                "type": "string",
                "description": "The clearing path of the operation",
                "enum": ["burn_entity", "burn_all", "burn_visits"]
            },
            {
                "key": "autoClear",
                "type": "boolean",
                "description": "Whether this was an automatic clear operation"
            }
        ]
    },

    "m_mac_fire_retrigger_in_20s": {
        "description": "Fired when the fire button is triggered again within 20 seconds of a previous fire",
        "owners": ["hanyutang-sandra"],
        "triggers": ["user_submitted"],
        "suffixes": ["daily_standard"],
        "parameters": ["appVersion", "pixelSource"]
    },

    // ==================== Web Cache Metrics ====================

    "m_mac_fire_burn_web_cache_error": {
        "description": "Fired when an error occurs while clearing web cache data",
        "owners": ["hanyutang-sandra"],
        "triggers": ["other"],
        "suffixes": ["daily_standard"],
        "parameters": ["appVersion", "pixelSource", "errorDomain", "errorCode"]
    },

    "m_mac_fire_burn_web_cache_duration": {
        "description": "Fired to report the duration of web cache clearing",
        "owners": ["hanyutang-sandra"],
        "triggers": ["other"],
        "parameters": [
            "appVersion",
            "pixelSource",
            {
                "key": "duration",
                "type": "integer",
                "description": "Duration of web cache clearing in milliseconds"
            }
        ]
    },

    "m_mac_fire_burn_web_cache_has_residue": {
        "description": "Fired when residual web cache data is detected after clearing",
        "owners": ["hanyutang-sandra"],
        "triggers": ["other"],
        "parameters": [
            "appVersion",
            "pixelSource",
            {
                "key": "step",
                "type": "string",
                "description": "The clearing step where residue was detected",
                "enum": [
                    "clear_file_cache",
                    "clear_device_hash_salts",
                    "clear_safely_removable_data_types",
                    "clear_local_storage_and_indexed_db_for_non_fireproof_domains",
                    "clear_cookies",
                    "clear_resource_load_statistics_database"
                ]
            }
        ]
    },

