{
    "m_mac_webkit_did_terminate": {
        "description": "Fires when a WebKit web content process terminates/crashes.",
        "owners": ["ayoy"],
        "triggers": ["exception"],
        "suffixes": ["daily_standard"],
        "parameters": [
            "appVersion",
            "pixelSource",
            {
                "key": "available_memory",
                "description": "Free operating system memory percentage",
                "type": "integer"
            },
            {
                "key": "available_diskspace",
                "description": "Available disk space percentage",
                "type": "number"
            },
            {
                "key": "os_version",
                "description": "Operating system version",
                "type": "string"
            },
            {
                "key": "is_duck_url",
                "description": "Whether the URL of a crashed tab is a 'duck' scheme URL",
                "type": "boolean"
            },
            "errorCode",
            "errorDomain",
            "underlyingErrorCode",
            "underlyingErrorDomain"
        ]
    },
    "m_mac_webkit_did_terminate_non_recoverable_aggregated": {
        "description": "Fires when multiple WebKit web content processes terminate/crash within a short time window (100ms), indicating burst crashes. Provides crash count for burst detection.",
        "owners": ["diegoreymendez"],
        "triggers": ["exception"],
        "suffixes": ["daily_standard"],
        "parameters": [
            "appVersion",
            "pixelSource",
            {
                "key": "tab_count",
                "description": "Number of tab crashes that occurred in the burst (within 100ms window)",
                "type": "string"
            }
        ]
    }
}
