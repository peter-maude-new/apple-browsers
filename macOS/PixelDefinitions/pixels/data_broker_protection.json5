        triggers: ['other'],
        suffixes: [],
        parameters: [
            'appVersion',
            'pixelSource',
            {
                key: 'isInternalUser',
                description: 'Whether the user is internal',
                type: 'string',
                enum: ['true', 'false'],
            },
            {
                key: 'tries',
                description: 'The number of tries it took for the action to complete',
                type: 'integer',
            },
            {
                key: 'data_broker',
                description: 'The name of the data broker that this action was operating on',
                type: 'string',
            },
            {
